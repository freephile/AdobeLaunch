{
	"name": "AdobeLaunch",
	"author": [
		"Greg Rundlett"
	],
	"url": "https://www.github.com/freephile/AdobeLaunch",
	"descriptionmsg": "adobelaunch-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\AdobeLaunch\\": "includes/"
	},
	"config": {
		"AdobeLaunchEnableFoo": {
			"description": "Enable the Foo feature.",
			"value": true
		},
		"AdobeLaunchVandalizeEachPage": {
			"description": "Write 'AdobeLaunch was here' on each page",
			"value": false
		}
	},
	"HookHandlers": {
		"AdobeLaunchHooks": {
			"class": "MediaWiki\\Extension\\AdobeLaunch\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "AdobeLaunchHooks"
	},
	"MessagesDirs": {
		"AdobeLaunch": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "AdobeLaunch/resources"
	},
	"ResourceModules": {
		"ext.boilerPlate": {
			"dependencies": [],
			"messages": [],
			"styles": [],
			"scripts": []
		}
	},
	"manifest_version": 2
}
